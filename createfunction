def studebt(name,gender,age,city='suzhou'):
	print('name:',name)
	print('gender:',gender)
	print('age:',age)
	print('city:',city)

def digui(n):
	if n==1:
		return 1
	return n*digui(n-1)

def weigui(num,product):
	if num==1:
		return product
	return weigui(num-1,num*product)


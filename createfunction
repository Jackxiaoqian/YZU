def qushu(number):
	friends=['wangyuchen','zhangxinyi','zhuzijie','atangge','yanyan','chenhu']
	if number>5:
		print("you do not have so many frineds")
	else:
		i=0
		L=[]
		while i<number:
			L.append(friends[i])
			i=i+1
		print(L)

def area_of_circle(r):
	s=r*r*3.1415
	print(s)
